<%= render 'layouts/running-text' %>
<%= render 'layouts/navbar' %>
<div class="container mt-3">
  <h1>Add a new bookmark</h1>
  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <%= f.submit class:"btn btn-primary" %>
  <% end %>
</div>
